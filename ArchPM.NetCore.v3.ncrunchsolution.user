<SolutionConfiguration>
  <Settings>
    <AutoEnable>True</AutoEnable>
    <CurrentEngineMode>Run all tests automatically [Global]</CurrentEngineMode>
    <StatusIndicatorSplitterDistance>-1%</StatusIndicatorSplitterDistance>
    <TestsWindowMenuOptions>
      <VerticalSplitMenuOption>false</VerticalSplitMenuOption>
      <ShowPassingTestsMenuOption>true</ShowPassingTestsMenuOption>
      <ShowFailingTestsMenuOption>true</ShowFailingTestsMenuOption>
      <ShowUnexecutedTestsMenuOption>false</ShowUnexecutedTestsMenuOption>
      <ShowIgnoredTestsMenuOption>true</ShowIgnoredTestsMenuOption>
    </TestsWindowMenuOptions>
    <TestsWindowSplitterDistance>-1%</TestsWindowSplitterDistance>
    <TestsWindowTreeLayout>
      <Column>
        <Name>Status</Name>
        <Width>150</Width>
      </Column>
      <Column>
        <Name>Name</Name>
        <Width>600</Width>
      </Column>
      <Column>
        <Name>LastExecutionTime</Name>
        <Width>180</Width>
      </Column>
      <Column>
        <Name>GridNode</Name>
        <Width>160</Width>
      </Column>
    </TestsWindowTreeLayout>
  </Settings>
</SolutionConfiguration>